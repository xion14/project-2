<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:visibility="visible"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary">
    <id.com.android.laundry.widget.BottomNavigationView
        android:layout_alignParentBottom="true"
        android:id="@+id/view_home_bottomnavigationview"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="@drawable/sp_widget_devider_navigation"
        android:outlineProvider="paddedBounds"
        app:elevation="@dimen/size_elevation_toolbar"
        app:itemIconTint="@drawable/sl_widget_tab_navigation"
        app:itemTextColor="@drawable/sl_widget_tab_navigation"
        app:layout_behavior="android.support.design.widget.BottomSheetBehavior"
        app:menu="@menu/menu_navigation"
        app:labelVisibilityMode="labeled"
        app:itemTextAppearanceActive="@style/BottomNavigationView"
        app:itemTextAppearanceInactive="@style/BottomNavigationView"
        tools:ignore="UnusedAttribute" />

    <FrameLayout
        android:id="@+id/view_home_container_framelayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior" />


</RelativeLayout>